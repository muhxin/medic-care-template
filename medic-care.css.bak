.nav ul {
	text-align:right;
	margin: 5px;
    padding: 34px 100px;
}

.nav li{
	float:none;
	display:inline-block;
}

.nav ul li:nth-child(1),
.nav ul li:nth-child(2),
.nav ul li:nth-child(3){
	float:left;
}

.nav ul li a{
	margin:0 35px;
	font-family:arial;
	text-decoration:none;
	color:slategrey;
	font-weight:bold;
}

.nav ul li:hover a{
	color:black;
	cursor:pointer;
}

